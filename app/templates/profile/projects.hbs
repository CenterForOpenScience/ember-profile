<main id="profile-projects">
  <section class="full">
    <div class="profile-box">
      <header>
        <h1 class="col-md-6">Projects</h1>
        <h2 class="col-md-2">Date Created</h2>
        <h2 class="col-md-2">Last Modified</h2>
        {{fa-icon "eye" class="col-md-1" title="Views"}}
        {{fa-icon "check" class="col-md-1" title="Registered"}}
      </header>
      <div class="profile-content no-padding">
        {{#projects-list model=model}}{{/projects-list}}
      </div>
    </div>
  </section>
</main>