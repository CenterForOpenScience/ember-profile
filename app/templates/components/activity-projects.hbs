<dl class="dl-horizontal project-stats">
  <dt>Total Projects:</dt>
  <dd class="spaced-list">{{model.total_projects}}</dd>
  <dt>Public Projects:</dt>
  <dd class="spaced-list">{{model.public_projects}}</dd>
  <dt>Projects this Year:</dt>
  <dd class="spaced-list">{{model.year_total_projects}}</dd>
  <dt>Total Components:</dt>
  <dd class="spaced-list">{{model.total_comp}}</dd>
  <dt>Public Components:</dt>
  <dd class="spaced-list">{{model.public_comp}}</dd>
  <dt>Components this Year:</dt>
  <dd class="spaced-list">{{model.year_total_comp}}</dd>
  <dt>Registrations:</dt>
  <dd class="spaced-list">{{model.registrations}}</dd>
  <dt>Files Added:</dt>
  <dd class="spaced-list">{{model.files}}</dd>
  <dt>Comments:</dt>
  <dd class="spaced-list">{{model.comments}}</dd>
  <dt>Most Contributed Project:</dt>
  <dd class="spaced-list">{{model.most_contributed}}</dd>
</dl>
