{{#each model as |project|}}
  {{#cp-panel class=(if project.private 'private')}}
    {{#cp-panel-toggle}}
      <div>
        <h1 class="{{if project.private 'col-md-10' 'col-md-6'}}">{{fa-icon "chevron-down"}}<b>{{project.name}}</b> ({{project.year}})</h1>
        {{#if project.private}}
          <button class="btn btn-default col-md-2">Make Public</button>
          {{#if project.registered}}
            {{fa-icon "check" class="col-md-1"}}
          {{/if}}
        {{else}}
          <div class="col-md-2 date-field vert-center"> {{project.created}} </div>
          <div class="col-md-2 date-field vert-center"> {{project.modified}} </div>
<!--         <h2 class="col-md-1">{{if project.components project.components.length 0}}</h2> !-->
<!--          <h2 class="col-md-1">{{if project.collaborators project.collaborators.length 0}}</h2> !-->
<!--          <h2 class="col-md-1">{{project.stats.files}}</h2> !-->
          <h2 class="col-md-1">{{project.stats.forks}}</h2>
          {{#if project.registered}}
            {{fa-icon "check" class="col-md-1"}}
          {{else}}
            {{fa-icon "times" class="col-md-1"}}
          {{/if}}
        {{/if}}
      </div>
    {{/cp-panel-toggle}}
    {{#if project.components}}
      {{#cp-panel-body}}
        <div class="col-md-12 descriptive-list"> <a href="/profile/collaborators">Contributors:</a> 
        {{#each project.collaborators as |collaborator|}}
          <a>{{collaborator.name}}, </a>
        {{/each}}
        </div>

        <div class="col-md-12 descriptive-list constraint-container">
          <div class="paragraph-container"> {{#read-more maxHeight='150px'}}{{{project.wiki}}}{{/read-more}} </div>
        </div>
<!--        <nav>
          {{#each project.components as |component|}}
            {{#link-to component.link}}{{component.name}}{{/link-to}}
          {{/each}}
        </nav>
!-->
      {{/cp-panel-body}}
    {{/if}}
  {{/cp-panel}}
{{/each}}